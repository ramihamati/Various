{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    },
    "Elastic": {
      "AuthType": "None",
      "Url": "http://192.168.1.103:9200",
      "Enabled": true,
      "IndexFormat": "test-logs-thirdgrpc-{0:yyyy.MM.dd}"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "EndpointDefaults": {
      "Protocols": "Http2"
    }
  },
  "HttpServer": {
    "Endpoints": [
      {
        "OS": "windows",
        "Host": "localhost",
        "Port": 25003,
        "Scheme": "https",
        "CertSource": "file",
        "CertStoreName": null,
        "CertStoreLocation": null,
        "CertFilePath": "certificates/prod.pfx",
        "CertPasswordSource": "configuration",
        "CertPassword": "secretpassword",
        "ListenProtocols": [ "Http2" ]
      }
    ]
  },
  "ElasticApm": {
    "SecretToken": "",
    "ServerUrls": "http://localhost:8200", //Set custom APM Server URL (default: http://localhost:8200)
    "ServiceName": "TestService-ThirdGrpc", //allowed characters: a-z, A-Z, 0-9, -, _, and space. Default is the entry assembly of the applicationapp.UseAllElasticApm(Configuration);
    "LogLevel": "Error"
  },
  "OpenTelemetry": {
    "Service": "ThirdGrpc",
    "Version": "1.0",
    "Jaeger": {
      "Host": "192.168.1.103",
      "Port": 6831
    },
    "ZipkinUrl": "http://192.168.1.103:9411/api/v2/spans"
  }
}
