{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    },
    "Elastic": {
      "AuthType": "None",
      "Url": "http://192.168.1.103:9200",
      "Enabled": true,
      "IndexFormat": "test-logs-firstapi-{0:yyyy.MM.dd}"
    }
  },
  "AllowedHosts": "*",
  "EndPoints": {
    "SecondApi": "https://192.168.1.103:25002"
  },
  "HttpClients": [
    {
      "Name": "GenericHttps",
      "Scheme": "https",
      "CertSource": "file",
      "CertStoreOnlyValidCertificates": false,
      "CertStoreFindValue": "certsearchcriteria",
      "CertFilePath": "certificates/prod.pfx",
      "CertPasswordSource": "configuration",
      "CertPassword": "secretpassword",
      "DefaultRequestVersion": "2.0"
    }
  ],
  "HttpServer": {
    "Endpoints": [
      {
        "OS": "windows",
        "Host": "localhost",
        "Port": 25001,
        "Scheme": "http",
        "ListenProtocols": [ "Http1" ]
      }
    ]
  },
  "ElasticApm": {
    "SecretToken": "",
    "ServerUrls": "http://localhost:8200", //Set custom APM Server URL (default: http://localhost:8200)
    "ServiceName": "TestService-FirstApi", //allowed characters: a-z, A-Z, 0-9, -, _, and space. Default is the entry assembly of the applicationapp.UseAllElasticApm(Configuration);
    "LogLevel": "Error"
  },
  "OpenTelemetry": {
    "Service": "FirstApi",
    "Version": "1.0",
    "Jaeger": {
      "Host": "192.168.1.103",
      "Port": 6831
    },
    "ZipkinUrl": "http://192.168.1.103:9411/api/v2/spans"
  }
}
